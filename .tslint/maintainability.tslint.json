{
  "rules": {
    "cyclomatic-complexity": [
      true,
      20
    ],
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "linebreak-style": [
      false
    ],
    "max-classes-per-file": [
      true,
      1
    ],
    "max-file-line-count": [
      true,
      300
    ],
    "max-line-length": [
      true,
      80
    ],
    "no-default-export": true,
    "no-mergeable-namespace": true,
    "no-require-imports": false,
    "no-trailing-whitespace": true
  }
}