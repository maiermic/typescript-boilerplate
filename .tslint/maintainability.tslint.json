{
  "rules": {
    "cyclomatic-complexity": [
      true,
      20
    ],
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "linebreak-style": [
      false
    ],
    "max-classes-per-file": [
      true,
      1
    ],
    "max-file-line-count": [
      true,
      300
    ],
    "no-trailing-whitespace": true
  }
}