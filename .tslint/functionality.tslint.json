{
  "rules": {
    "ban": [false],
    "curly": true,
    "forin": true,
    "import-blacklist": [true],
    "label-position": true,
    "no-arg": true,
    "no-eval": true,
    "no-unsafe-finally": true,
    "no-var-keyword": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ]
  }
}